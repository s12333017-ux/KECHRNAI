{
    "appId": "com.smartkitchen.ai",
    "appName": "Smart Kitchen AI",
    "webDir": ".",
    "bundledWebRuntime": false,
    "server": {
        "androidScheme": "https"
    },
    "android": {
        "allowMixedContent": true,
        "captureInput": true,
        "webContentsDebuggingEnabled": true,
        "backgroundColor": "#059669"
    },
    "plugins": {
        "SplashScreen": {
            "launchShowDuration": 2000,
            "backgroundColor": "#059669",
            "androidScaleType": "CENTER_CROP",
            "showSpinner": false,
            "androidSpinnerStyle": "large",
            "spinnerColor": "#ffffff"
        },
        "StatusBar": {
            "style": "dark",
            "backgroundColor": "#059669"
        }
    }
}